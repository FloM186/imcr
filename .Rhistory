install.packages("C:/Users/moret/Downloads/_documents/GitHub/projetR/imcr_0.0.0.9000.tar.gz", repos = NULL, type = "source")
library(imcr)
data(iris)
iris.cr <- scale(iris[,-5],center=T,scale=T)
d.iris <- dist(iris.cr)
cah.ward <- hclust(d.iris,method="ward.D2")
groupes.cah <- cutree(cah.ward,k=4)
test_value(iris[,-5], groupes.cah, show_graph = TRUE, digits=3)
?test_value
?corr_coef
?effect_size
